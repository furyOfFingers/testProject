@import '../../Assets/variables.styl';

.table-container
  display flex
  margin 10px
  border 1px solid black

.table-container th
  padding 6px 10px
  background-color $color-dividing-line

.table-container td
  padding 6px 10px

.data-body-table td
  text-align center

.table-container tr
  &:nth-child(odd)
    background-color #f2f2f2

.table-container-header
  text-align center